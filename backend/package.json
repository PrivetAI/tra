{
  "name": "videoharvester-backend",
  "version": "0.1.0",
  "private": true,
  
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:api": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:worker:youtube": "ts-node-dev --respawn --transpile-only src/workers/youtube.ts",
    "dev:worker:tiktok": "ts-node-dev --respawn --transpile-only src/workers/tiktok.ts",
    "dev:worker:instagram": "ts-node-dev --respawn --transpile-only src/workers/instagram.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "worker:youtube": "node dist/workers/youtube.js",
    "worker:tiktok": "node dist/workers/tiktok.js",
    "worker:instagram": "node dist/workers/instagram.js"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "bull": "^4.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "mongoose": "^8.5.1",
    "morgan": "^1.10.0",
    "googleapis": "^140.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.11.30",
    "@types/morgan": "^1.9.7",
    "@types/uuid": "^9.0.7",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
